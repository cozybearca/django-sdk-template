// this file is used for vscode editor, for the config file used by tsc, see webpack.config.js
{
    "compilerOptions": {
        "target": "es5",
        "moduleResolution": "node",
        "sourceMap": true,
        "allowJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noEmit": true,
        "baseUrl": ".",
        "paths": {
            "*": [
                // * replaces module name
                "*"
            ]
        }
    }
}
